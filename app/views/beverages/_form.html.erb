<%= form_with(model: @beverage) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :calories %>
    <%= f.number_field :calories %>
  </div>
  <div>
    <%= f.label :image, 'Foto' %>
    <%= f.file_field :image %>
  </div>
  <div>
    <%= f.submit 'Enviar'%>
  </div>
<% end %>